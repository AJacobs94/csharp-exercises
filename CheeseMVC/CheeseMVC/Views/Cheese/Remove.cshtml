
@{
    Layout = null;
}
    <form method="post">
       
        <select name="Remove">
            @foreach (KeyValuePair<string, string> cheese in ViewBag.cheeses)
            {
                <option value="removeCheese">@cheese.Key</option>
            }
        </select>
        <input type="submit" value="Remove Cheese" />

    </form>
